{
  "name": "h5p-types",
  "version": "1.5.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc --noEmit false --out index.js --module \"amd\"",
    "combine": "esr combine-types.ts",
    "package": "npm run combine && npm run build",
    "publish-to-npm": "npm run package && np",
    "test": "echo 'No tests'",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.33.0",
    "eslint-plugin-prettier": "^4.2.1",
    "np": "^7.6.3",
    "prettier": "^2.8.3",
    "prettier-config": "github:boyum/prettier-config",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/jquery": "^3.5.16"
  },
  "np": {
    "releaseDraft": true
  },
  "prettier": "prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyum/h5p-types.git"
  }
}
