{
  "name": "h5p-json-schemas",
  "version": "1.0.0",
  "description": "Schemas for H5P related JSON files",
  "scripts": {
    "generate:library": "typescript-json-schema index.d.ts H5PLibrary -o public/library.schema.json",
    "generate:semantics": "typescript-json-schema index.d.ts H5PSemantics -o public/semantics.schema.json",
    "build": "npm-run-all --parallel generate:**",
    "deploy": "vercel --prod",
    "start": "vercel dev"
  },
  "keywords": [
    "h5p",
    "json",
    "schema"
  ],
  "author": {
    "name": "Sindre BÃ¸yum",
    "url": "https://sinre.dev"
  },
  "license": "ISC",
  "devDependencies": {
    "h5p-types": "*",
    "npm-run-all": "^4.1.5",
    "typescript-json-schema": "^0.58.1",
    "vercel": "^31.0.2"
  }
}
