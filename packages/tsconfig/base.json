{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "ES2021.String"],
    "moduleResolution": "Node",
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "declaration": false
  },
  "extends": "@tsconfig/strictest/tsconfig.json"
}
