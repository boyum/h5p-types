{
  "name": "h5p-generate-configs",
  "version": "0.4.2",
  "description": "h5p-generate-configs CLI",
  "types": "build/types/types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyum/h5p-types.git"
  },
  "homepage": "https://github.com/boyum/h5p-types/tree/main/packages/h5p-generate-configs#readme",
  "bin": {
    "h5p-generate-configs": "bin/h5p-generate-configs"
  },
  "scripts": {
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles ./src/templates/* ./build/templates",
    "build": "npm run clean-build && npm run compile && npm run copy-templates",
    "prepublishOnly": "npm run build",
    "test": "jest && npm link && npm run demo:semantics && npm run demo:library",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "demo:semantics": "h5p-generate-configs s --type-definition demo/semantics.ts --out demo/semantics.json -t demo/TranslationKey.ts",
    "demo:library": "h5p-generate-configs l --type-definition demo/library.ts --out demo/library.json",
    "publish-to-npm": "release-it",
    "lint:check": "eslint .",
    "lint:fix": "npm run lint:check -- --fix"
  },
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "docs",
    "bin",
    "tsconfig.json"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "gluegun": "^5.1.2",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.1",
    "copyfiles": "^2.4.1",
    "eslint-config-base": "*",
    "h5p-types": "*",
    "jest": "^29.6.1",
    "release-it": "^16.1.0",
    "ts-jest": "^29.1.1",
    "tsconfig-base": "*",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "extends": "base"
  }
}
